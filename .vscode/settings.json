{
  "cSpell.words": [
    "Blurple",
    "crosspost",
    "crosspostable",
    "DBNAME",
    "emojies",
    "haileybot",
    "Hypesquad",
    "moderatable",
    "Wenhook",
    "wtimeout"
  ],
  "json.schemas": [
    {
      "fileMatch": ["src/config.json"],
      "url": "/config.schema.json"
    }
  ],
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "biomejs.biome",
  "[typescript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "editor.codeActionsOnSave": {
    "quickfix.biome": "explicit",
    "source.organizeImports.biome": "explicit"
  }
}
