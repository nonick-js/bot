{
  "name": "nonick.js-bot",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --inspect src/index.ts",
    "start": "node dist/index.js",
    "build": "drizzle-kit migrate && tsc -p . && pnpm run copy-assets",
