    "@types/node-cron": "^3.0.11",
    "nodemon": "^3.1.7",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.2.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4",
    "typescript-transform-paths": "^3.5.1"
  },
  "dependencies": {
    "@akki256/discord-interaction": "^3.1.1",
    "@napi-rs/canvas": "0.1.59",
    "adm-zip": "^0.5.16",
    "axios": "^1.7.7",
    "dayjs": "^1.11.13",
    "discord-api-types": "0.37.83",
    "discord.js": "^14.15.3",
