{
  "name": "nonick.js-bot",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --inspect src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc -p . && pnpm run copy-assets",
    "prepare": "ts-patch install",
    "copy-assets": "shx cp -r src/fonts dist/fonts"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@swc/core": "^1.11.22",
    "@swc/helpers": "^0.5.17",
    "@types/adm-zip": "^0.5.7",
    "@types/node": "22.15.2",
    "@types/node-cron": "^3.0.11",
    "nodemon": "^3.1.10",
    "shx": "^0.4.0",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.3.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-transform-paths": "^3.5.5"
  },
  "dependencies": {
    "@akki256/discord-interaction": "^3.1.1",
    "@napi-rs/canvas": "^0.1.69",
    "adm-zip": "^0.5.16",
    "axios": "^1.9.0",
    "dayjs": "^1.11.13",
    "discord-api-types": "0.38.1",
    "discord.js": "^14.19.1",
    "dotenv": "^16.5.0",
    "drizzle-kit": "^0.31.0",
    "drizzle-orm": "^0.43.1",
    "drizzle-zod": "^0.7.1",
    "i18next": "^25.0.1",
    "node-cron": "^3.0.3",
    "pg": "^8.15.6",
    "ts-node-dev": "^2.0.0",
    "zod": "^3.24.3",
    "zod-i18n-map": "^2.27.0"
  },
  "overrides": {
    "discord-api-types": "0.38.1"
  },
  "pnpm": {
    "overrides": {
      "discord-api-types": "0.38.1"
    },
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "@swc/core",
      "esbuild"
    ]
  }
}
